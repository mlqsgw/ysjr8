.user_login_bar .user-lr-box-left{ width:287px; padding:0; margin:0 auto; margin-top:50px; }

.user_login_bar .user-lr-box-left .field .holder_tip{ position:absolute;left:44px; top:0; height:44px; line-height:44px; width:234px;width:254px; padding:0 10px; color:#8596B0; font-size:14px;}
.suser_login_bar .user-lr-box-left .field input.ipttxt{ margin:0; border:none; height:29px; line-height:29px; background:url(../images/input-bg-all.png) no-repeat 0 0; width:234px;color:#8596B0; font-size:14px; font-weight:normal;}

.user_login_bar .user-lr-box-left .autologin{margin-left: 0px; padding:0px; height:auto;}
.user_login_bar .user-lr-box-left .verify input.ipttxt,
.register_box  .user-lr-box-left .verify input.ipttxt
{ margin:0; width:70px;padding: 10px 5px; background:#fff; text-align:center;}

.user-lr-box-left .field input.ipttxt {
    border: 1px solid #C5C5C5;
    border-radius: 2px;
    background: #FFF url("../images/input-bg-all.png") no-repeat scroll 0% 0%;
    color: #3B3636;
    height: 22px;
    line-height: 22px;
    font-size: 13px;
    font-family: "Microsoft YaHei";
    width: 230px;
    padding: 10px 5px 10px 50px;
}
.user-lr-box-left .email input.ipttxt{ background-position:0 -245px; }
.user-lr-box-left .password input.ipttxt{ background-position:0 -94px; }
.user-lr-box-left .mobile input.ipttxt{ background-position:0 -175px; }
.user-lr-box-left .weibo input.ipttxt{ background:#fff; padding-left:10px; width:270px }
.user_login_bar .user-lr-box-left .email input.ipttxt{ background-position:0 0; }

.user-lr-box-left .login-submit-btn {
background:none;
text-indent:0;
color: #fff;
font: 18px "Microsoft YaHei";
border-radius: 2px;
overflow: hidden;
float: left;
border: none;
cursor: pointer;
height: 40px;
line-height: 40px;
width: 165px;
background-color:#E47F2B;
}
.user-lr-box-left .app_login_box a{ progid:DXImageTransform.Microsoft.BasicImage(grayscale=1); -webkit-filter:grayscale(100%);}
.user-lr-box-left .app_login_box a:hover{ progid:DXImageTransform.Microsoft.BasicImage(grayscale=0); -webkit-filter:grayscale(0%); }


.user_login_bar .user-lr-box-left .app_login_box{ padding:0px;float:right;}
